{
  "name": "byscripts/static-entity-bundle",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "description": "Add Symfony3 ParamConverter and FormType to StaticEntity",
  "license": "MIT",
  "authors": [
    {
      "name": "Thierry Goettelmann",
      "email": "thierry@byscripts.info"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/dependency-injection": ">=3.0",
    "symfony/config": ">=3.0",
    "symfony/http-kernel": ">=3.0",
    "symfony/http-foundation": ">=3.0",
    "symfony/form": ">=3.0",
    "symfony/options-resolver": ">=3.0",
    "sensio/framework-extra-bundle": ">=3.0",
    "byscripts/static-entity": ">=3.0"
  },
  "require-dev": {
    "symfony/finder": ">=3.0",
    "symfony/browser-kit": ">=3.0",
    "symfony/expression-language": ">=3.0",
    "symfony/validator": ">=3.0",
    "symfony/twig-bundle": ">=3.0"
  },
  "autoload": {
    "psr-4": {
      "Byscripts\\Bundle\\StaticEntityBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Byscripts\\BundleTest\\StaticEntityBundle\\": "tests/Fixtures/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  }
}
